        "@types/nunjucks": "^3.1.4",
        "bit-field": "1.3.6",
        "clone": "^2.1.2",
        "json5": "2.1.3",
        "nunjucks": "^3.2.0",
        "onml": "2.1.0",
        "showdown": "1.9.1",
        "showdown-highlight": "2.1.8",
        "state-machine-cat": "9.2.2",
        "svg.js": "2.7.1",
        "svgdom": "^0.0.21",
        "temp": "0.9.4",
        "wavedrom": "2.6.8",
        "web-tree-sitter": "^0.16.2"
    },
    "devDependencies": {
        "@types/assert": "^1.5.6",
        "@types/expect": "^24.3.0",
        "@types/mocha": "^9.1.1",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "assert": "^2.0.0",
        "eslint": "^8.17.0",
